<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="logboard.decompiler.pattern_matcher" class="%logboard.decompiler.pattern_matcher.class%" public="true"/>

        <service id="logboard.file_storage_parameters" class="%logboard.parameters.file_storage.class%" public="true">
            <argument type="service" id="filesystem"/>
        </service>

        <service id="logboard.finder.file_storage" class="%logboard.finder.file_storage.class%" public="true"/>

    </services>

</container>
